<ehcache>
	<diskStore path="d:/cache/ehcache"/>
	<defaultCache
        maxElementsInMemory="1000"
        eternal="false"
        timeToIdleSeconds="300000"
        timeToLiveSeconds="600000"
        overflowToDisk="true"
        memoryStoreEvictionPolicy="LFU"
        />
  <cache name="DEFAULT_CACHE"
        maxElementsInMemory="1000"
        eternal="false"
        timeToIdleSeconds="300000"
        timeToLiveSeconds="600000"
        overflowToDisk="true"
        memoryStoreEvictionPolicy="LFU"
        />
</ehcache>
