<?xml version="1.0" encoding="UTF-8"?>
<ehcache>

	<diskStore path="d:/tmp/cache/file/local/" />
	
	<defaultCache maxElementsInMemory="100" memoryStoreEvictionPolicy="LRU" eternal="true"
		overflowToDisk="true" diskPersistent="true" />
	<cache name="fileImageCache" overflowToDisk="true" eternal="true" diskPersistent="true" maxElementsInMemory="10" memoryStoreEvictionPolicy="LRU"/>
	<cache name="fileCache" overflowToDisk="true" eternal="true" diskPersistent="true" maxElementsInMemory="10" memoryStoreEvictionPolicy="LRU"/>
</ehcache>
